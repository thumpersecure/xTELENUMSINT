{
  "manifest_version": 3,
  "name": "TELESPOT-NUMSINT",
  "version": "1.1.0",
  "description": "Phone number OSINT tool - Generate multiple phone number formats and search across Google, Bing, and DuckDuckGo",
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "https://www.google.com/*",
    "https://www.bing.com/*",
    "https://duckduckgo.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "TELESPOT-NUMSINT"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
