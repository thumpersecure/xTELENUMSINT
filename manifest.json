{
  "manifest_version": 3,
  "name": "TELESPOT-NUMSINT",
  "version": "1.3.0",
  "description": "Phone number OSINT tool - Generate multiple phone number formats with Smart Search, auto pattern extraction, and reporting",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://www.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "TELESPOT-NUMSINT"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
